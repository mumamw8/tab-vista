{
  "name": "tab-vista-extension",
  "description": "Display bookmarks on your new tab page.",
  "private": true,
  "version": "0.0.1.5",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
