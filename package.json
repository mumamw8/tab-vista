    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.514.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.10",
    "tldts": "^7.0.8"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@wxt-dev/module-react": "^1.1.3",
    "daisyui": "^5.0.43",
    "typescript": "^5.8.3",
    "wxt": "^0.20.6"
  }
